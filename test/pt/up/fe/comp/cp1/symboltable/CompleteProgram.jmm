// A comprehensive test case with multiple features
import java.io;
import utils.MathUtils;

class CompleteProgram extends BaseProgram {
    // Fields
    int counter;
    boolean active;
    int[] data;
    
    // Method that calculates a value based on parameters
    public int calculate(int a, boolean flag) {
        int result;
        int temp;
        
        temp = 0;
        if (flag) {
            temp = a * 2;
        } else {
            temp = a / 2;
        }
        
        result = temp + counter;
        active = 10 < result;
        
        return result;
    }
    
    // Method that processes arrays
    public int[] processArray(int[] input) {
        int[] result;
        int i;
        
        result = new int[input.length];
        i = 0;
        
        while (i < input.length) {
            result[i] = input[i] * 2;
            i = i + 1;
        }
        
        return result;
    }
    
    // Method with varargs
    public int sum(int... numbers) {
        int total;
        int i;
        
        total = 0;
        i = 0;
        
        while (i < numbers.length) {
            total = total + numbers[i];
            i = i + 1;
        }
        
        return total;
    }
    
    // Main method
    public static void main(String[] args) {
        CompleteProgram program;
        int value;
        int[] numbers;
        int sumResult;
        
        program = new CompleteProgram();
        // program.counter = 5;
        // program.active = true;
        
        value = program.calculate(10, true);
        
        numbers = [1, 2, 3, 4, 5];
        // program.data = program.processArray(numbers);
        
        sumResult = program.sum(10, 20, 30);
    }
}
