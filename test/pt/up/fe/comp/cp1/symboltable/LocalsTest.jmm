// Tests that local variables are correctly added to the symbol table
import java.io;

class LocalsTest {
    // Methods with different local variables
    
    public int testMethod(int param) {
        int[] arrVar;
        int simpleVar;
        
        arrVar = new int[5];
        simpleVar = param + 10;
        
        return simpleVar;
    }
    
    public static void main(String[] args) {
        int intVar;
        boolean boolVar;
        LocalsTest objVar;
        
        intVar = 42;
        boolVar = true;
        objVar = new LocalsTest();
    }
}
